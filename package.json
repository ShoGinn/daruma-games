{
  "name": "daruma-games",
  "version": "1.18.0",
  "private": true,
  "description": "Algodaruma Server Bot for NFT games!",
  "license": "MIT",
  "main": "build/esm/main.js",
  "module": "build/esm/main.js",
  "type": "module",
  "scripts": {
    "build": "pnpm clean && pnpm generate-version && pnpm build:esm",
    "build:esm": "tsc",
    "clean": "rimraf build coverage",
    "dev": "pnpm build && pnpm start",
    "generate-version": "genversion -e -s ./src/version.ts",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "npx is-ci || npx husky install",
    "prettier": "prettier --write src tests",
    "pretty:lint": "pnpm prettier && pnpm lint:fix",
    "schema": "npx mikro-orm-esm schema:update",
    "start": "node build/esm/main.js",
    "test:coverage": "pnpm jest --maxWorkers=1 --reporters=default --coverage --verbose",
    "test:coverage-local": "pnpm jest --reporters=default --coverage",
    "test:find-open-handles": "pnpm jest --detectOpenHandles",
    "test:debug": "pnpm jest --runInBand",
    "test": "pnpm jest",
    "type-check": "tsc --noemit",
    "dep-typesync": "node ./node_modules/typesync/bin/typesync",
    "dep-update": "pnpm up",
    "dep-check-unused": "node ./node_modules/depcheck/bin/depcheck.js --ignores='rimraf,@commitlint/cli,@commitlint/config-conventional,@types/jest,ts-jest,ts-jest-resolver'"
  },
  "dependencies": {
    "@discordx/importer": "~1.2.3",
    "@discordx/pagination": "~3.4.1",
    "@discordx/utilities": "~5.2.1",
    "@mikro-orm/better-sqlite": "^5.8.8",
    "@mikro-orm/core": "^5.8.8",
    "@mikro-orm/migrations": "^5.8.8",
    "@mikro-orm/sql-highlighter": "~1.0.1",
    "algosdk": "^2.6.0",
    "axios": "^1.5.1",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "cron-parser": "^4.9.0",
    "cron-validator": "^1.3.1",
    "dayjs": "^1.11.10",
    "discord.js": "~14.13.0",
    "discordx": "~11.7.6",
    "dotenv": "~16.3.1",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "memoizer-ts": "^1.0.1",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.1",
    "rate-limiter-flexible": "^3.0.0",
    "reflect-metadata": "~0.1.13",
    "toad-scheduler": "^3.0.0",
    "tslib": "^2.6.2",
    "tsyringe": "~4.8.0",
    "typescript-retry-decorator": "^2.4.2",
    "winston": "^3.11.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@faker-js/faker": "^8.1.0",
    "@jest/globals": "^29.7.0",
    "@mikro-orm/cli": "^5.8.8",
    "@mikro-orm/seeder": "^5.8.8",
    "@shoginn/discordjs-mock": "^1.0.0",
    "@tsconfig/strictest": "^2.0.2",
    "@types/convict": "^6.1.4",
    "@types/convict-format-with-validator": "^6.0.3",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "@types/mitm": "^1.3.5",
    "@types/node": "~20.8.4",
    "@types/node-schedule": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "depcheck": "^1.4.6",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "genversion": "^3.1.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "mitm": "^1.7.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "5.2.2",
    "typesync": "^0.11.1"
  },
  "typesync": {
    "ignorePackages": [
      "eslint",
      "table",
      "eslint-config-prettier",
      "http-status-codes",
      "is-ci"
    ]
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.8.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "build/src/mikro-orm.config.js"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
