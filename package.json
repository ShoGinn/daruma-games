{
    "name": "daruma-games",
    "version": "1.12.1",
    "private": true,
    "description": "Algodaruma Server Bot for NFT games!",
    "license": "MIT",
    "type": "module",
    "main": "./build/main.js",
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rimraf build",
        "dev": "node --loader ts-node/esm/transpile-only src/Main.ts",
        "lint": "eslint . --ext .ts --max-warnings 0",
        "lint:fix": "eslint . --ext .ts --fix",
        "prettier": "prettier --config .prettierrc --write .",
        "pretty:lint": "npm run prettier && npm run lint:fix",
        "start": "node build/main.js",
        "update_deps": "npx npm-check-updates"
    },
    "dependencies": {
        "@discordx/importer": "~1.1.10",
        "@discordx/pagination": "~3.4.0",
        "@discordx/utilities": "~5.2.1",
        "@mikro-orm/better-sqlite": "^5.6.7",
        "@mikro-orm/core": "^5.6.7",
        "@mikro-orm/migrations": "^5.6.7",
        "@mikro-orm/mysql": "^5.6.7",
        "@mikro-orm/postgresql": "^5.6.7",
        "@mikro-orm/sql-highlighter": "~1.0.1",
        "algosdk": "^2.0.0",
        "axios": "^1.2.3",
        "cron-parser": "^4.7.1",
        "cron-validator": "^1.3.1",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.7",
        "discord.js": "~14.7.1",
        "discordx": "~11.5.2",
        "dotenv": "~16.0.3",
        "http-status-codes": "^2.2.0",
        "immutable": "^4.2.2",
        "node-cache": "^5.1.2",
        "node-schedule": "^2.1.1",
        "rate-limiter-flexible": "^2.4.1",
        "reflect-metadata": "~0.1.13",
        "toad-scheduler": "^2.2.0",
        "tsyringe": "~4.7.0",
        "typescript-retry-decorator": "^2.2.0",
        "winston": "^3.8.2",
        "winston-transport": "^4.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.2",
        "@commitlint/config-conventional": "^17.4.2",
        "@mikro-orm/cli": "^5.6.7",
        "@types/node": "~18.11.18",
        "@types/node-schedule": "^2.1.0",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "~8.6",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^39.6.7",
        "eslint-plugin-simple-import-sort": "^9.0.0",
        "eslint-plugin-unicorn": "^45.0.2",
        "prettier": "^2.8.3",
        "rimraf": "^4.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "engines": {
        "node": ">=16.16.0",
        "npm": ">=8.11.0"
    },
    "mikro-orm": {
        "configPaths": [
            "./src/mikro-orm.config.ts",
            "./build/mikro-orm.config.js"
        ]
    }
}
