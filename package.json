{
    "name": "daruma-games",
    "version": "1.8.0",
    "private": true,
    "description": "Algodaruma Server Bot for NFT games!",
    "license": "MIT",
    "type": "module",
    "main": "./build/main.js",
    "scripts": {
        "build": "tsc",
        "dev": "cross-env NODE_ENV=development node --loader ts-node/esm/transpile-only src/Main.ts",
        "lint": "eslint . --ext .ts --max-warnings 0",
        "lint:fix": "eslint . --ext .ts --fix",
        "migration:create": "npx mikro-orm migration:create",
        "migration:down": "npx mikro-orm migration:down",
        "migration:up": "npx mikro-orm migration:up",
        "prettier": "prettier --config .prettierrc --write .",
        "pretty:lint": "npm run prettier && npm run lint:fix",
        "serve": "node build/main.js",
        "start": "cross-env NODE_ENV=production node build/main.js",
        "update_deps": "npx npm-check-updates"
    },
    "dependencies": {
        "@discordx/importer": "~1.1.10",
        "@discordx/pagination": "~3.4.0",
        "@discordx/utilities": "~5.2.0",
        "@mikro-orm/core": "^5.6.0",
        "@mikro-orm/migrations": "^5.6.0",
        "@mikro-orm/mysql": "^5.6.0",
        "@mikro-orm/sql-highlighter": "~1.0.1",
        "algosdk": "^1.24.1",
        "axios": "^1.2.1",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.7",
        "discord.js": "~14.7.1",
        "discordx": "~11.5.2",
        "dotenv": "~16.0.3",
        "http-status-codes": "^2.2.0",
        "immutable": "^4.1.0",
        "rate-limiter-flexible": "^2.4.1",
        "reflect-metadata": "~0.1.13",
        "rxeta": "~1.1.2",
        "toad-scheduler": "^2.1.0",
        "tsyringe": "~4.7.0",
        "typescript-retry-decorator": "^2.0.6",
        "winston": "^3.8.2",
        "winston-transport": "^4.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@mikro-orm/cli": "^5.6.0",
        "@types/node": "~18.11.15",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "~8.5",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.6.4",
        "eslint-plugin-unicorn": "^45.0.2",
        "prettier": "^2.8.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "engines": {
        "node": ">=16.16.0",
        "npm": ">=8.11.0"
    },
    "mikro-orm": {
        "configPaths": [
            "./src/mikro-orm.config.ts",
            "./build/mikro-orm.config.js"
        ]
    }
}
