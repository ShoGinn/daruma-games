{
  "name": "daruma-games",
  "version": "1.5.1",
  "description": "A Bot to play Daruma Games",
  "license": "MIT",
  "main": "build/main.js",
  "scripts": {
    "prepare": "husky install",
    "build": "npm run build:clean && npm run build:compile && npm run build:plugins && npm run type:check",
    "build:start": "npm run build && npm run start",
    "build:compile": "swc src -d build -D && rimraf build/plugins/**/node_modules/",
    "build:clean": "rimraf build/",
    "build:plugins": "installoop --rootDir=./build/plugins",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec node --trace-deprecation -r @swc-node/register src/main.ts",
    "dev:start": "cross-env NODE_ENV=production node -r @swc-node/register src/main.ts",
    "dev:compile": "swc src -w -d build",
    "i18n": "typesafe-i18n --no-watch",
    "type:check": "tsc --pretty --skipLibCheck --noEmit",
    "migration:create": "npx mikro-orm migration:create",
    "migration:up": "npx mikro-orm migration:up",
    "migration:down": "npx mikro-orm migration:down",
    "plugins:install": "installoop --rootDir=./src/plugins",
    "postinstall": "npm run plugins:install",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --config .prettierrc --write .",
    "pretty:lint": "npm run prettier && npm run lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run i18n && npm run prettier:lint"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "src/i18n/**/!(i18n-types.ts)"
    ]
  },
  "dependencies": {
    "@discordx/importer": "~1.1.10",
    "@discordx/pagination": "~3.4.0",
    "@discordx/utilities": "~5.2.0",
    "@mikro-orm/cli": "^5.5.3",
    "@mikro-orm/core": "^5.5.3",
    "@mikro-orm/mariadb": "^5.5.3",
    "@mikro-orm/migrations": "^5.5.3",
    "@mikro-orm/mongodb": "^5.5.3",
    "@mikro-orm/mysql": "^5.5.3",
    "@mikro-orm/postgresql": "^5.5.3",
    "@mikro-orm/sql-highlighter": "~1.0.1",
    "@mikro-orm/sqlite": "^5.5.3",
    "@tsed/common": "^7.7.1",
    "@tsed/core": "^7.7.1",
    "@tsed/di": "^7.7.1",
    "@tsed/exceptions": "^7.7.1",
    "@tsed/json-mapper": "^7.7.1",
    "@tsed/platform-express": "^7.7.1",
    "@tsed/schema": "^7.7.1",
    "@tsed/swagger": "^7.7.1",
    "algosdk": "^1.23.2",
    "axios": "^1.2.0",
    "boxen": "5.1.2",
    "case": "~1.6.3",
    "chalk": "4.1.2",
    "cron": "~2.1.0",
    "cron-validator": "~1.3.1",
    "cross-env": "~7.0.3",
    "dayjs": "^1.11.6",
    "discord-logs": "~2.2.1",
    "discord-oauth2": "~2.10.1",
    "discord.js": "~14.7.1",
    "discordx": "~11.5.0",
    "dotenv": "~16.0.3",
    "fast-folder-size": "~1.7.1",
    "image-hash": "~5.3.1",
    "imgur": "~2.2.0",
    "installoop": "^1.1.0",
    "limiter": "^2.1.0",
    "node-os-utils": "~1.3.7",
    "ora": "5.4.1",
    "pidusage": "^3.0.2",
    "reflect-metadata": "~0.1.13",
    "rentry-pastebin": "~1.2.3",
    "rxeta": "~1.1.2",
    "saveqlite": "~1.1.2",
    "semver": "^7.3.8",
    "socket.io-client": "^4.5.4",
    "stacktrace-parser": "^0.1.10",
    "ts-toolbelt": "^9.6.0",
    "tsyringe": "~4.7.0",
    "typesafe-i18n": "^5.17.1",
    "typescript-retry-decorator": "^2.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@swc-node/register": "~1.5.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.21",
    "@types/better-sqlite3": "^7.6.2",
    "@types/cron": "~2.0.0",
    "@types/node": "~18.11.10",
    "@types/node-os-utils": "~1.3.0",
    "@types/pidusage": "~2.0.2",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "chokidar": "~3.5.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-unicorn": "^45.0.1",
    "husky": "^8.0.2",
    "nodemon": "~2.0.20",
    "npm-run-all": "~4.1.5",
    "prettier": "^2.8.0",
    "rimraf": "~3.0.2",
    "ts-node": "~10.9.1",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "~4.1.1",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=8.0.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts",
      "./build/mikro-orm.config.js"
    ]
  },
  "tscordTemplate": true,
  "volta": {
    "node": "16.17.0"
  }
}
