{
    "name": "daruma-games",
    "version": "1.6.0",
    "description": "A Bot to play Daruma Games",
    "license": "MIT",
    "type": "module",
    "main": "./build/Main.js",
    "scripts": {
        "prepare": "husky install",
        "build": "tsc",
        "dev": "node --loader ts-node/esm/transpile-only src/Main.ts",
        "serve": "node build/Main.js",
        "lint": "eslint . --ext .ts --max-warnings 0",
        "lint:fix": "eslint . --ext .ts --fix",
        "start": "node build/Main.js",
        "migration:create": "npx mikro-orm migration:create",
        "migration:up": "npx mikro-orm migration:up",
        "migration:down": "npx mikro-orm migration:down",
        "prettier": "prettier --config .prettierrc --write .",
        "pretty:lint": "npm run prettier && npm run lint:fix",
        "update_deps": "npx npm-check-updates"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run i18n && npm run prettier:lint"
        }
    },
    "dependencies": {
        "@discordx/importer": "~1.1.10",
        "@discordx/pagination": "~3.4.0",
        "@discordx/utilities": "~5.2.0",
        "@mikro-orm/cli": "^5.5.3",
        "@mikro-orm/core": "^5.5.3",
        "@mikro-orm/migrations": "^5.5.3",
        "@mikro-orm/mysql": "^5.5.3",
        "@mikro-orm/sql-highlighter": "~1.0.1",
        "algosdk": "^1.24.0",
        "axios": "^1.2.1",
        "case": "~1.6.3",
        "cron": "~2.1.0",
        "cron-validator": "~1.3.1",
        "cross-env": "~7.0.3",
        "dayjs": "^1.11.7",
        "discord.js": "~14.7.1",
        "discordx": "~11.5.0",
        "dotenv": "~16.0.3",
        "fast-folder-size": "~1.7.1",
        "http-status-codes": "^2.2.0",
        "image-hash": "~5.3.1",
        "immutable": "^4.1.0",
        "installoop": "^1.1.0",
        "limiter": "^2.1.0",
        "node-os-utils": "~1.3.7",
        "pidusage": "^3.0.2",
        "reflect-metadata": "~0.1.13",
        "rxeta": "~1.1.2",
        "saveqlite": "~1.1.2",
        "semver": "^7.3.8",
        "socket.io-client": "^4.5.4",
        "stacktrace-parser": "^0.1.10",
        "ts-toolbelt": "^9.6.0",
        "tsyringe": "~4.7.0",
        "typescript-retry-decorator": "^2.0.6",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@types/better-sqlite3": "^7.6.2",
        "@types/cron": "~2.0.0",
        "@types/node": "~18.11.11",
        "@types/node-os-utils": "~1.3.0",
        "@types/pidusage": "~2.0.2",
        "@types/semver": "^7.3.13",
        "@typescript-eslint/eslint-plugin": "^5.45.1",
        "@typescript-eslint/parser": "^5.45.1",
        "chokidar": "~3.5.3",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "~8.5",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.6.4",
        "eslint-plugin-unicorn": "^45.0.1",
        "husky": "^8.0.2",
        "nodemon": "~2.0.20",
        "npm-run-all": "~4.1.5",
        "prettier": "^2.8.0",
        "rimraf": "~3.0.2",
        "ts-node": "~10.9.1",
        "tsc-alias": "^1.8.1",
        "tsconfig-paths": "~4.1.1",
        "typescript": "^4.9.3"
    },
    "engines": {
        "node": ">=16.9.0",
        "npm": ">=8.0.0"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./mikro-orm.config.ts",
            "./build/mikro-orm.config.js"
        ]
    },
    "tscordTemplate": true,
    "volta": {
        "node": "16.17.0"
    }
}
