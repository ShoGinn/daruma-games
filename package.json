{
  "name": "daruma-games",
  "version": "1.21.0",
  "private": true,
  "description": "Algodaruma Server Bot for NFT games!",
  "license": "MIT",
  "main": "build/esm/main.js",
  "module": "build/esm/main.js",
  "type": "module",
  "scripts": {
    "build": "pnpm clean && pnpm generate-version && pnpm build:esm",
    "build:esm": "tsc",
    "clean": "rimraf build coverage",
    "dev": "pnpm build && pnpm start",
    "generate-nfd-types": "openapi-typescript https://api.nf.domains/info/openapi3.yaml -o ./src/types/api-generated/nfdomains.d.ts && prettier --write ./src/types/api-generated/nfdomains.d.ts && eslint --fix ./src/types/api-generated/nfdomains.d.ts",
    "generate-indexer-types": "openapi-typescript https://openapi.algonode.cloud/indexer2.oas3.json -o ./src/types/api-generated/algoindexer.d.ts && prettier --write ./src/types/api-generated/algoindexer.d.ts && eslint --fix ./src/types/api-generated/algoindexer.d.ts",
    "generate-mainnet-types": "openapi-typescript https://openapi.algonode.cloud/algod.oas3.json -o ./src/types/api-generated/mainnet.d.ts && prettier --write ./src/types/api-generated/mainnet.d.ts && eslint --fix ./src/types/api-generated/mainnet.d.ts",
    "generate-all-types": "pnpm generate-nfd-types && pnpm generate-indexer-types && pnpm generate-mainnet-types",
    "generate-version": "genversion -e -s ./src/version.ts",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "npx is-ci || npx husky install",
    "prettier": "prettier --write src tests",
    "pretty:lint": "pnpm prettier && pnpm lint:fix",
    "start": "node build/esm/main.js",
    "test:coverage": "pnpm jest --reporters=default --coverage",
    "test:find-open-handles": "pnpm jest --detectOpenHandles",
    "test:debug": "pnpm jest --runInBand",
    "test": "pnpm jest",
    "type-check": "tsc --noemit",
    "dep-typesync": "node ./node_modules/typesync/bin/typesync",
    "dep-update": "pnpm up",
    "dep-check-unused": "node ./node_modules/depcheck/bin/depcheck.js --ignores='rimraf,@commitlint/cli,@commitlint/config-conventional,@types/jest,ts-jest,ts-jest-resolver'"
  },
  "dependencies": {
    "@discordx/importer": "~1.3.0",
    "@discordx/pagination": "~3.5.0",
    "@discordx/utilities": "~6.0.0",
    "algosdk": "^2.7.0",
    "axios": "^1.6.2",
    "bottleneck": "^2.19.5",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "cron-parser": "^4.9.0",
    "cron-validator": "^1.3.1",
    "dayjs": "^1.11.10",
    "decorate-all": "^1.2.1",
    "discord.js": "~14.14.1",
    "discordx": "~11.8.1",
    "dotenv": "~16.3.1",
    "http-status-codes": "^2.3.0",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "memoizer-ts": "^1.0.1",
    "mongoose": "^8.0.1",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.1",
    "random-js": "^2.1.0",
    "reflect-metadata": "~0.1.13",
    "toad-scheduler": "^3.0.0",
    "tslib": "^2.6.2",
    "tsyringe": "~4.8.0",
    "winston": "^3.11.0",
    "winston-transport": "^4.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.2",
    "@commitlint/config-conventional": "^18.4.2",
    "@faker-js/faker": "^8.3.1",
    "@jest/globals": "^29.7.0",
    "@shoginn/discordjs-mock": "^1.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/strictest": "^2.0.2",
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.201",
    "@types/mitm": "^1.3.7",
    "@types/node": "~20.9.1",
    "@types/node-schedule": "^2.1.3",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "genversion": "^3.1.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "mitm": "^1.7.2",
    "mongodb-memory-server": "^9.1.1",
    "nyc": "^15.1.0",
    "openapi-typescript": "7.0.0-next.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-jest-resolver": "^2.0.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "typesync": "^0.11.1"
  },
  "typesync": {
    "ignorePackages": [
      "eslint",
      "table",
      "eslint-config-prettier",
      "http-status-codes",
      "is-ci"
    ]
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.8.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
